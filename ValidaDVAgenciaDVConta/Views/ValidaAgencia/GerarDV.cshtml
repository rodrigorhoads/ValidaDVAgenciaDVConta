@model ContaBancaria
@{
    ViewData["Title"] = "GerarDV";
}

<h2>GerarDV</h2>

<div class="row">
    <div class="form-group col-sm-2">
        <label for="Banco">Banco</label>
        <input class="form-control" type="text" asp-for="Banco"  />
    </div>
    <div class="form-group col-sm-2">
        <label for="Agencia">Agencia</label>
        <input class="form-control" type="text" asp-for="Agencia" />
    </div>
    <div class="form-group col-sm-1">
        <label for="DvAgencia">DV</label>
        <input class="form-control" type="text" asp-for="DvAgencia" />
    </div>
    <div class="form-group col-sm-2">
        <label for="txtConta">Conta</label>
        <input class="form-control" type="text" asp-for="Conta"  />
    </div>
    <div class="form-group col-sm-1">
        <label for="txtDvConta">DV</label>
        <input class="form-control" type="text" asp-for="DvConta"/>
    </div>
</div>

